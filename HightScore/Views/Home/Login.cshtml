@using HightScore.Entities.Model.Concrete
@model IEnumerable<MetaUser> 

<a class="btn btn-primary" asp-controller="CreateUser" asp-action="Index">Create User</a>

<table class="table table-sm table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Username</th>
            <th>Email</th>
            <th>Number</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Id</td>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.PhoneNumber</td>
                <td>
                    <a href="" class=" btn btn-primary btn-sm">Edit</a>
                    <a href="" class=" btn btn-danger btn-sm">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>